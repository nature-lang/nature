=== test_pi_test
--- main.n
fn pi_test():f64 {
    var rounds = 1000000000
    var x = 1.0
    var pi = 1.0
    f64 stop = (rounds + 2) as float

    for f64 i = 2.0; i <= stop; i+=1 {
        x = -x
        pi += x / (2.0 * i - 1.0)
    }

    pi *= 4.0
    return pi
}

fn main() {
    var v = pi_test()
    println(v)
    assert(v > 3.14 && v < 3.15)
}

