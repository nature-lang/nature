=== test_linked
--- main.n
type node_t = struct {
    ref<node_t>? next
    int id
}

fn main() {
    var n1 = new node_t(id = 1)
    var n2 = new node_t(id = 2, next = n1)
    var n3 = new node_t(id = 3, next = n2)

    ref<node_t>? tail_option = n3
    for tail_option is ref<node_t> {
        var tail = tail_option as ref<node_t>

        println(tail.id)
        tail_option = tail.next
    }
}

