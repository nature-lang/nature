fn boom():int {
    var test1 = fn() {
        println('in test1')
    }

    var test2 = fn() {
        println('in test2')
    }

    test1()

    if 1 > 2 {
        return 12
    }

    for int i = 0; i < 10; i+=1 {
        if i % 2 == 0 {
            test1()
        } else {
            test2()
        }

        if i == 9 {
            break
        }
    }

    test2()

    return 2333
}

fn main() {
    boom()
}