type Pair<T,U> = struct {
	T first;
	U second;
};

fn Pair<T,U>.get_first(&self): T {
	return self.first;
}

fn Pair<T,U>.get_second(&self): U {
	return self.second;
}

type Message = struct {
	string content;
	i32 id;
};

fn Message.get_content(&self): string {
	return self.content;
}

