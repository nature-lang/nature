=== test_env_vec
--- main.n
import co
import runtime

fn get_list_fn():fn():[int] {
    var list = [1, 2, 3]

    return fn():[int] {
        // env assign
        list[0] = 11
        list[1] = 22
        list[2] = 33

        // list assign
        list = [list[0] * 2, list[1] * 2, list[2] * 2]

        return list
    }
}

fn main() {
    // start server
    var list_fn = get_list_fn()
    var list = list_fn()

    // gc
    runtime.gc()
    co.sleep(1000)

    println(list[0], list[1], list[2])
}

--- output.txt
22 44 66

