fn main() {
    i32 s0 = 70000
    f64 s1 = 3.1415926
    u8 s2 = 125
    i16 s3 = -1250

    var a = 3 * s2 as int + s3 as int // -875
    var b = 3 as i16 / (s2 as i16 + s3) // -0.00266667 = 0
    var c = s1 * s2 as f64 + s0 as f64 // 70392.699075
    var d = s0 * (8 / 5) as i32 * 3 // 210000
    var e = 3 + 2 - 5 + 8 + 1000 - 1000 // 8
    var f = 3 * 2 / 5 * 8 * 2 * 3 / 12 // 4.8 = 4

    println(a, b, c, d, e, f) //-875070392.69907521000084
}