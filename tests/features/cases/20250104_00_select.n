fn main():void! {
    var ch1 = chan<int>.new(1)
    var ch2 = chan<string>.new()

    for int i = 0; i < 2; i+=1 {
        select{
            ch2.on_recv() -> msg {
                println('ch2 on_recv msg:', msg)
            }
            ch1.on_recv() -> msg {
                println('ch1 on_recv msg:', msg)
            }
            ch1.on_send(23312) -> {
                println('ch1 on_send success')
            }
        }
    }
}