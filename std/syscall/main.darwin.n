#linkid getcwd
fn libc_getcwd(void_ptr path, uint size):void_ptr

#linkid strlen
fn libc_strlen(void_ptr str):uint

fn getcwd():string {
    var buf = vec_new<u8>(1024, 0) // 1024 是 path max

    // 这不能是自己调用自己了，冲突的yip
    void_ptr raw_path = libc_getcwd(buf.ref() as void_ptr, buf.len() as uint)
    var len = libc_strlen(raw_path) as i64

    buf = buf.slice(0, len - 1) // -1 is remove '\0'
    return buf as string
}

fn kill(int pid, int sig) {
    call6(SYS_KILL, pid as void_ptr, sig as void_ptr, 0, 0, 0, 0)
