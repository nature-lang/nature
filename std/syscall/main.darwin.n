fn call6(int number, void_ptr a1, void_ptr a2, void_ptr a3, void_ptr a4, void_ptr a5, void_ptr a6):int {
    return syscall_call6(number, a1, a2, a3, a4, a5, a6)
}

fn open(string filename, int flags, u32 perm):int {
    return call6(SYS_OPEN, filename.ref(), flags as void_ptr, perm as void_ptr, 0, 0, 0)
}


fn read(int fd, void_ptr buf, int len):int {
    return call6(SYS_READ, fd as void_ptr, buf, len as void_ptr, 0, 0, 0)
}

fn readlink(string file, [u8] buf):int {
    return call6(SYS_READLINK, file.ref(), buf.ref(), buf.len() as void_ptr, 0, 0, 0)
}

fn write(int fd, void_ptr buf, int len):int {
    return call6(SYS_WRITE, fd as void_ptr, buf, len as void_ptr, 0, 0, 0)
}

fn close(int fd) {
    call6(SYS_CLOSE, fd as void_ptr, 0, 0, 0, 0, 0)
}

fn unlink(string path) {
    call6(SYS_UNLINK, path.ref(), 0, 0, 0, 0, 0)
}

fn seek(int fd, int offset, int whence):int {
    return call6(SYS_LSEEK, fd as void_ptr, offset as void_ptr, whence as void_ptr, 0, 0, 0)
}

#linkid fork
fn libc_fork():int

fn fork():int {
    return libc_fork()
    // return call6(SYS_FORK, 0, 0, 0, 0, 0, 0)
}

// path is full path
// argv[0] = /proc/:pid/comm
fn exec(string path, [string] argv, [string] envp) {
    return syscall_exec(path, argv, envp)
}

