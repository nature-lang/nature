fn wait(int pid, int option):(int, int) {
    var status = 0
    int result = call6(SYS_WAIT4, pid as void_ptr, &status as void_ptr, option as void_ptr, 0, 0, 0)
    if result == -1 {
        throw libc_strerror()
    }
    return (result, status)
}

