fn bind<T>(int sockfd, T addr) {
    var len = @sizeof(T) as void_ptr
    call6(SYS_BIND, sockfd as void_ptr, addr as void_ptr, len, 0, 0, 0)
}

fn bind6(int sockfd, sockaddr_in6 addr) {
    var len = @sizeof(sockaddr_in6) as void_ptr
